{
   "author": "Sinew Software Systems",
   "background": {
      "page": "background.html"
   },
   "browser_action": {
      "default_icon": {
         "19": "data/images/toolbar-chrome.png",
         "38": "data/images/toolbar-chrome@2x.png"
      }
   },
   "content_scripts": [ {
      "all_frames": true,
      "css": [ "data/commonCSS/fillStyle.css" ],
      "js": [ "data/commonJS/mousetrap.min.js", "data/commonJS/mousetrap-global-bind.min.js", "data/commonJS/login.js", "data/commonJS/changePassword.js", "data/commonJS/creditcard.js", "data/chromeJS/chromeController.js" ],
      "matches": [ "http://*/*", "https://*/*" ],
      "run_at": "document_start"
   } ],
   "description": "Enpass Password Manager extension for Google Chrome",
   "homepage_url": "http://enpass.io/",
   "icons": {
      "128": "data/images/icon-128.png",
      "16": "data/images/icon-16.png",
      "32": "data/images/icon-32.png",
      "48": "data/images/icon-48.png",
      "64": "data/images/icon-64.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Enpass Password Manager",
   "options_ui": {
      "open_in_tab": true,
      "page": "data/chromeUI/option.html"
   },
   "permissions": [ "activeTab", "tabs", "http://*/*", "https://*/*", "contextMenus", "webNavigation", "storage" ],
   "short_name": "Enpass",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "5.3.3"
}
